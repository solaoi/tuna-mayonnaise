{
  "name": "tuna-mayonnaise-ui",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": "18",
    "npm": ">=8.3"
  },
  "dependencies": {
    "@monaco-editor/react": "4.6.0",
    "file-saver": "^2.0.5",
    "handlebars": "^4.7.8",
    "js-sql-parser": "1.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.4.1",
    "react-modal": "3.16.1",
    "regenerator-runtime": "0.14.1",
    "rete": "2.0.3",
    "rete-area-plugin": "2.0.2",
    "rete-auto-arrange-plugin": "^2.0.1",
    "rete-connection-path-plugin": "^2.0.3",
    "rete-connection-plugin": "2.0.1",
    "rete-connection-reroute-plugin": "^2.0.1",
    "rete-context-menu-plugin": "^2.0.1",
    "rete-history-plugin": "^2.0.1",
    "rete-minimap-plugin": "^2.0.2",
    "rete-react-render-plugin": "0.3.1",
    "use-interval": "1.4.0"
  },
  "devDependencies": {
    "@types/pug": "2.0.10",
    "@types/react": "18.2.63",
    "@types/react-dom": "18.2.20",
    "@types/react-modal": "3.16.3",
    "@vitejs/plugin-react": "4.2.1",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "rollup-plugin-visualizer": "5.12.0",
    "stylelint": "16.2.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recommended": "14.0.0",
    "stylelint-order": "6.0.4",
    "stylelint-prettier": "5.0.0",
    "typescript": "5.3.3",
    "vite": "5.1.5",
    "vite-plugin-externals": "0.6.2"
  },
  "overrides": {
    "rete-context-menu-plugin": {
      "lodash-es": "4.17.21"
    }
  },
  "scripts": {
    "prepare": "cd .. && husky install ui/.husky",
    "lint:css": "stylelint --fix src/**/*.css",
    "lint:js": "eslint --fix src/**/*.{js,jsx}",
    "format:js": "prettier --write src/**/*.{js,jsx}",
    "fix": "npm run format:js && npm run lint:js && npm run lint:css",
    "dev": "vite",
    "vite-build": "vite build",
    "build": "npm run vite-build && rm -rf ../command/cmd/static/* && mv dist/* ../command/cmd/static/ && touch ../command/cmd/static/.gitkeep && cd ../command/ && go build -o ../tuna && cd ../ui/",
    "build:win": "npm run vite-build && rm -rf ../command/cmd/static/* && mv dist/* ../command/cmd/static/ && touch ../command/cmd/static/.gitkeep && cd ../command/ && GOOS=windows GOARCH=amd64 go build -o ../tuna.exe && cd ../ui/",
    "analyze": "vite build --mode analyze",
    "typecheck": "tsc"
  },
  "lint-staged": {
    "*.css": "stylelint --fix",
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
